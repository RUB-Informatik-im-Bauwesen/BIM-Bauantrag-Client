  <xs:simpleType name="Codelist.Genehmigungsfreistellung">
    <xs:annotation>
      <xs:appinfo>
        <codeliste>
          <nameLang>Genehmigungsfreistellungsverfahren Ergebnis</nameLang>
          <nameKurz>Genehmigungsfreistellungsverfahren Ergebnis</nameKurz>
